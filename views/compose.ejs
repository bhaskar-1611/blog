<%- include("partials/header") %>
<h1>Compose</h1>
<form action="/compose" method="post">
    <div class="form-group">
        <label>Title</label>
        <textarea class="form-control" name="titleInput" cols="100" rows="1" class="composeCon " style="resize: none;"></textarea>
        <label>Post</label>
        <textarea class="form-control" name="postInput" cols="100" rows="5" class="composeCon" style="resize: none;"></textarea>
    </div>
    <button class="btn btn-primary" type="submit" name="button" style="background-color: rgb(4, 4, 162); color: white;" class="publishBtn">Publish</button>    
</form>
<%- include("partials/footer") %>